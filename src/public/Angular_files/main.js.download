  color: #000;
  box-shadow: 0 0 25px var(--accent-color);
  transform: scale(1.05);
}
button:disabled {
  opacity: 0.5;
  cursor: not-allowed;
  box-shadow: none;
}
select {
  background: var(--tile-bg);
  border: 1px solid var(--border-color);
  appearance: none;
  background-image: url("data:image/svg+xml,<svg xmlns='http://www.w3.org/2000/svg' fill='%23fff' viewBox='0 0 24 24'><path d='M7 10l5 5 5-5z'/></svg>");
  background-repeat: no-repeat;
  background-position: right 15px center;
  background-size: 16px;
}
.error {
  color: var(--error-color);
  font-weight: bold;
  text-align: center;
  display: none;
  margin-top: 10px;
}
.error.show {
  display: block;
}
#display-container {
  display: none;
  max-width: 850px;
  width: 100%;
  margin-top: 40px;
}
.solution-content {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 35px;
  width: 100%;
}
.display-board-container {
  width: 100%;
  max-width: 420px;
}
#display-board {
  width: 100%;
  aspect-ratio: 1 / 1;
  background: var(--tile-empty-bg);
  border: 2px solid var(--border-color);
  border-radius: 12px;
  overflow: hidden;
  position: relative;
  box-shadow: 0 0 15px rgba(0, 224, 255, 0.2);
}
.display-tile {
  background: var(--tile-bg);
  border-radius: 12px;
  color: var(--text-color);
  font-weight: 700;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  transition: all 0.4s ease;
}
.display-tile.empty {
  background: none;
}
.results-panel {
  width: 100%;
  max-width: 420px;
  background: rgba(255, 255, 255, 0.04);
  border: 1px solid var(--border-color);
  border-radius: 15px;
  padding: 20px;
  box-shadow: 0 0 20px rgba(255, 75, 145, 0.2);
}
.result-item {
  display: flex;
  justify-content: space-between;
  padding: 10px 0;
  border-bottom: 1px solid var(--border-color);
}
.result-item span:first-child {
  color: var(--text-muted);
}
.result-item span:last-child {
  font-weight: bold;
  color: var(--accent-color);
}
#reset-button {
  width: 100%;
  background: var(--success-color);
  box-shadow: 0 0 20px rgba(62, 229, 119, 0.4);
}
#reset-button:hover {
  background: #00d06f;
  transform: scale(1.05);
}
.controls button {
  min-width: 150px;
  white-space: nowrap;
  font-size: 1.05rem;
  padding-left: 18px;
  padding-right: 18px;
}
@keyframes fadeIn {
  from {
    opacity: 0;
    transform: translateY(-10px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}
@keyframes slideUp {
  from {
    opacity: 0;
    transform: translateY(20px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}
@keyframes shake {
  0%, 100% {
    transform: translateX(0);
  }
  25% {
    transform: translateX(-6px);
  }
  75% {
    transform: translateX(6px);
  }
}
@media (min-width: 768px) {
  .solution-content {
    flex-direction: row;
    align-items: flex-start;
    justify-content: space-between;
  }
  .results-panel {
    flex: 1;
    margin-left: 20px;
  }
}
@media (min-width: 1200px) {
  .solver-container {
    width: 55vw;
    max-width: none;
  }
}
@media (max-width: 600px) {
  .solver-container {
    width: 92vw;
    max-width: none;
    padding: 20px;
  }
}
#display-container {
  display: flex !important;
  flex-direction: column;
  align-items: center;
}
#display-container .results-panel {
  width: 100%;
  max-width: 520px;
  margin: 10px auto;
  text-align: center;
}
#reset-button {
  width: auto;
  min-width: 160px;
  padding: 12px 24px;
  margin: 18px auto 0 auto;
  display: block;
}
#display-container .puzzle-grid {
  justify-content: center;
  max-width: 720px;
  width: 100%;
  margin: 0 auto;
}
.extra-options {
  display: inline-flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.puzzle-grid2 {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.puzzle-cell2 {
  max-width: 60px;
  min-height: 30px;
  color: var(--text-muted);
  background: var(--tile-bg);
  border-radius: 5px;
}
.puzzle-cell2:hover,
.extra-options-seidel:hover {
  border: 2px solid var(--accent-color);
  box-shadow: 0 0 10px var(--accent-color);
}
.puzzle-cell2:focus,
.extra-options-seidel:focus {
  border: 2px solid var(--accent-color);
  box-shadow: 0 0 10px var(--accent-color);
  outline: none;
}
.select-method {
  width: 170px;
}
span {
  color: var(--text-muted);
}
.extra-options-seidel {
  width: 60px;
  padding: 8px;
  border-radius: 8px;
  max-width: 60px;
  min-height: 30px;
  color: var(--text-muted);
  background: var(--tile-bg);
  margin-top: 15px;
}
.results-values {
  width: 100%;
  padding: 5px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-evenly;
}
.results-values span {
  padding: 10px;
}
.table-container {
  display: block;
  overflow-x: auto;
  width: 100%;
  background-color: inherit;
  margin-top: 20px;
  align-items: center;
  justify-content: center;
  border-radius: 10px;
}
.table {
  border: 1px solid var(--border-color);
  border-radius: 10px;
  border-collapse: separate;
  border-spacing: 0;
  overflow: hidden;
  white-space: nowrap;
  width: max-content;
}
.table th,
.table td {
  padding: 12px 15px;
  text-align: center;
  border: 1px solid var(--border-color);
  min-width: 80px;
}
.col {
  position: sticky;
  left: 0;
  background-color: var(--tile-bg);
  z-index: 10;
  border-right: 1px solid var(--border-color);
  box-shadow: 2px 0 5px rgba(0, 0, 0, 0.1);
}
thead tr th.col {
  z-index: 11;
}
.equations-container {
  margin-top: 20px;
  background: inherit;
  border-radius: 8px;
  padding: 20px;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.05);
}
.equations-list {
  display: flex;
  flex-direction: row;
  gap: 15px;
}
.equations-container h3 {
  margin-top: 0;
  color: #F9F9F9;
  font-size: 1.1rem;
  border-bottom: 2px solid var(--accent-color);
  display: inline-block;
  padding-bottom: 10px;
  margin-bottom: 15px;
}
.equation-row {
  font-family: "Times New Roman", serif;
  font-size: 1.3rem;
  margin-bottom: 12px;
  padding: 10px;
  background-color: inherit;
  border-radius: 6px;
  border-left: 4px solid var(--accent-color);
}
#results-dashboard {
  width: 100%;
  max-width: 800px;
  margin: 0 auto;
  text-align: center;
}
.metrics-container {
  display: flex;
  justify-content: center;
  gap: 15px;
  margin: 20px 0;
  flex-wrap: wrap;
}
.metric-card {
  background: rgba(255, 255, 255, 0.05);
  border: 1px solid var(--border-color);
  border-radius: 12px;
  padding: 15px 25px;
  display: flex;
  flex-direction: column;
  min-width: 120px;
  transition: transform 0.2s;
}
.metric-card:hover {
  background: rgba(255, 255, 255, 0.08);
  transform: translateY(-3px);
  border-color: var(--accent-color);
}
.metric-card .label {
  font-size: 0.85rem;
  color: var(--text-muted);
  text-transform: uppercase;
  letter-spacing: 1px;
}
.metric-card .value {
  font-size: 1.5rem;
  font-weight: bold;
  color: #fff;
  margin-top: 5px;
}
.vector-display {
  background: var(--card-bg);
  border-radius: 16px;
  padding: 20px;
  margin-top: 25px;
  border: 1px solid var(--border-color);
  box-shadow: inset 0 0 20px rgba(0, 0, 0, 0.2);
}
.vector-grid {
  display: flex;
  flex-direction: column;
  gap: 10px;
  max-height: 300px;
  overflow-y: auto;
  padding-right: 5px;
}
.vector-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: rgba(255, 255, 255, 0.03);
  padding: 12px 20px;
  border-radius: 8px;
  border-left: 3px solid var(--primary-color);
}
.vector-item:nth-child(even) {
  background: rgba(255, 255, 255, 0.06);
  border-left: 3px solid var(--accent-color);
}
.var-name {
  font-weight: bold;
  color: var(--text-muted);
}
.var-value {
  font-family: "Courier New", monospace;
  font-size: 1.2rem;
  color: #fff;
  font-weight: bold;
}
.animate-up {
  animation: slideUp 0.5s ease-out;
}
.vector-grid::-webkit-scrollbar {
  width: 6px;
}
.vector-grid::-webkit-scrollbar-track {
  background: rgba(255, 255, 255, 0.02);
}
.vector-grid::-webkit-scrollbar-thumb {
  background: var(--tile-bg);
  border-radius: 3px;
}
.step-description {
  background: rgba(255, 255, 255, 0.05);
  border-left: 4px solid var(--accent-color);
  border-radius: 4px;
  padding: 15px 25px;
  font-family:
    "Courier New",
    Courier,
    monospace;
  font-weight: 900;
  font-size: 1.2rem;
  color: #ffffff;
  letter-spacing: 0.5px;
  display: inline-block;
  margin-top: 15px;
  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
}
.lu-container {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: flex-start;
  gap: 20px;
  margin-top: 40px;
  margin-bottom: 30px;
  width: 100%;
  flex-wrap: wrap;
  padding: 20px;
  background: rgba(255, 255, 255, 0.02);
  border-radius: 20px;
  border: 1px dashed var(--border-color);
}
.matrix-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  flex: 1;
  min-width: 300px;
}
.lu-separator {
  font-size: 3rem;
  font-weight: bold;
  color: var(--text-muted);
  align-self: center;
  margin: 0 10px;
}
.matrix-wrapper .puzzle-grid {
  width: 100%;
  max-width: 500px;
}
.popup {
  position: fixed;
  opacity: 0;
  visibility: hidden;
  pointer-events: none;
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 2000;
  transition: opacity 0.3s ease, visibility 0.3s ease;
  overflow: scroll;
}
.popup.show {
  opacity: 1;
  visibility: visible;
  pointer-events: auto;
}
.popup-menu {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  min-width: 450px;
  min-height: 250px;
  background-color: var(--tile-bg);
  border-radius: 10px;
  border: 1px solid var(--border-color);
  transform: scale(0.7);
  transition: transform 0.3s cubic-bezier(0.18, 0.89, 0.32, 1.28), box-shadow 0.2s ease-in-out;
}
.popup.show .popup-menu {
  transform: scale(1);
}
.popup-menu:hover {
  box-shadow: 0 0 30px var(--accent-color);
}
.popup-controlls {
  display: flex;
  flex-direction: row;
  width: 100%;
  padding: 30px 20px 10px 10px;
  align-items: center;
}
.popup-menu input {
  margin-left: auto;
  border-radius: 10px;
  text-align: center;
  color: var(--text-muted);
  background-color: #403f3f;
  min-height: 30px;
  max-height: 40px;
  transition: all 0.1s ease-in-out;
}
.popup-menu input:hover {
  box-shadow: 0 0 30px var(--error-color);
  border: 1px solid var(--error-color);
}
.continuebtn {
  display: flex;
  width: 100%;
  margin: auto 10px 20px 0;
  justify-content: center;
}
.initial-guess {
  width: 100%;
  padding-bottom: 20px;
}
